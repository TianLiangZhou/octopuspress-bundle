{
    "name": "meshell/octopuspress-bundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.2",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "ckfinder/ckfinder-symfony-bundle": "dev-main",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^3.2",
        "doctrine/dbal": "^4.0",
        "mobiledetect/mobiledetectlib": "^3.74",
        "spatie/flysystem-dropbox": "^2.0.6",
        "symfony/asset": "7.*",
        "symfony/console": "7.*",
        "symfony/doctrine-bridge": "7.*",
        "symfony/dotenv": "7.*",
        "symfony/filesystem": "7.*",
        "symfony/form": "7.*",
        "symfony/framework-bundle": "7.*",
        "symfony/http-client": "7.*",
        "symfony/mailer": "7.*",
        "symfony/messenger": "7.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "7.*",
        "symfony/redis-messenger": "7.*",
        "symfony/security-bundle": "7.*",
        "symfony/security-csrf": "7.*",
        "symfony/twig-bundle": "7.*",
        "symfony/validator": "7.*",
        "symfony/yaml": "7.*",
        "symfony/rate-limiter": "7.*",
        "beberlei/doctrineextensions": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14.4",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-doctrine": "^1",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5.23",
        "staabm/phpstan-dba": "^0.2.42",
        "symfony/browser-kit": "7.*",
        "symfony/css-selector": "7.*",
        "symfony/maker-bundle": "^1.45",
        "symfony/phpunit-bridge": "^5.4.11",
        "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "OctopusPress\\Bundle\\": "src/"
        },
        "exclude-from-classmap": [
            "*/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OctopusPress\\Bundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/TianLiangZhou/ckfinder-symfony-bundle"
        }
    ]
}
