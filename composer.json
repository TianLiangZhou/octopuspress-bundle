{
    "name": "meshell/octopuspress-bundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.1",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "ckfinder/ckfinder-symfony-bundle": "^4.0.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.14.1",
        "mobiledetect/mobiledetectlib": "^3.74",
        "sensio/framework-extra-bundle": "^6.2.9",
        "spatie/flysystem-dropbox": "^2.0.6",
        "symfony/asset": "6.4.* || 7.*",
        "symfony/console": "6.4.* || 7.*",
        "symfony/doctrine-bridge": "6.4.* || 7.*",
        "symfony/dotenv": "6.4.* || 7.*",
        "symfony/filesystem": "6.4.* || 7.*",
        "symfony/form": "6.4.* || 7.*",
        "symfony/framework-bundle": "6.4.* || 7.*",
        "symfony/http-client": "6.4.* || 7.*",
        "symfony/mailer": "6.4.* || 7.*",
        "symfony/messenger": "6.4.* || 7.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "6.4.* || 7.*",
        "symfony/proxy-manager-bridge": "6.4.* || 7.*",
        "symfony/redis-messenger": "6.4.* || 7.*",
        "symfony/security-bundle": "6.4.* || 7.*",
        "symfony/security-csrf": "6.4.* || 7.*",
        "symfony/twig-bundle": "6.4.* || 7.*",
        "symfony/validator": "6.4.* || 7.*",
        "symfony/yaml": "6.4.* || 7.*",
        "symfony/rate-limiter": "6.4.* || 7.*",
        "beberlei/doctrineextensions": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14.4",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-doctrine": "^1",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5.23",
        "staabm/phpstan-dba": "^0.2.42",
        "symfony/browser-kit": "6.4.* || 7.*",
        "symfony/css-selector": "6.4.* || 7.*",
        "symfony/maker-bundle": "^1.45",
        "symfony/phpunit-bridge": "^5.4.11"
    },
    "autoload": {
        "psr-4": {
            "OctopusPress\\Bundle\\": "src/"
        },
        "exclude-from-classmap": [
            "*/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Octopus\\PressBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
        },
        "post-install-cmd": [
            "@auto-scripts"
        ]
    }
}
